<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>servlet.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>about</servlet-name>
        <servlet-class>servlet.about</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>missionvisioncms</servlet-name>
        <servlet-class>servlet.missionvisioncms</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>editabout</servlet-name>
        <servlet-class>servlet.editabout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>courses</servlet-name>
        <servlet-class>servlet.courses</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>coursescms</servlet-name>
        <servlet-class>servlet.coursescms</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>carouselcms</servlet-name>
        <servlet-class>servlet.carouselcms</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>editcourses</servlet-name>
        <servlet-class>servlet.editcourses</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>about</servlet-name>
        <url-pattern>/about</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>missionvisioncms</servlet-name>
        <url-pattern>/cms/missionvisioncms</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>editabout</servlet-name>
        <url-pattern>/cms/editabout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>courses</servlet-name>
        <url-pattern>/courses</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>coursescms</servlet-name>
        <url-pattern>/cms/coursescms</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>carouselcms</servlet-name>
        <url-pattern>/cms/carouselcms</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>editcourses</servlet-name>
        <url-pattern>/cms/editcourses</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
